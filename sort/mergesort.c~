int *MergeSort(int v[], int n)
{
  int q, *v_left,*v_right;
  q=n/2;
  if(q<1)
    return v;
  v_left = ArraySplit(v,0,q);
  v_right = ArraySplit(v,q+1,n-1);
  MergeSort(v_left,q);
  MergeSort(v_right,n-q);
  return Merge(v_left, v_right);
}
int *Merge(int v_left[], int v_right[])
{

}

int *ArraySplit(int v[],int p, int r)
{
  int i,*a;
  for(i=0;i<r-p+1;i++)
    {
      a[i]=v[p];
      p++;
    }
  return a;
}
  
