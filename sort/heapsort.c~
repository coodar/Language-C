int PARENT(int i)
{
  return i/2;
}
int LEFT(int i)
{
  return 2*i;
}
int RIGHT(int i)
{
  return 2*i+1;
}
void MAX_HEAPIFY(int v[], int i, int n)
{
  int l,r,largest;
  l = LEFT(i);
  r = RIGHT(i);
  if(l<=n-1 && v[l]>v[i])
    largest = l;
  else
    largest = i;
  if(r<=n-1 && v[r]>v[largest])
    largest = r;
  if(largest != i)
    
}
